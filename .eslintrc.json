{
    "plugins": ["import"],
    "rules": {
        "import/order": [
            "error",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index"
                ],
                "newlines-between": "always",
                "alphabetize": { "order": "asc", "caseInsensitive": true }
            }
        ],
        "no-unused-vars": ["error", { "varsIgnorePattern": "^_" }],
        "no-unused-imports": "error"
    }
}
